{
  "manifest_version": 3,
  "name": "GraphQeLves",
  "version": "1.0.0",
  "version_name": "1.0.0",
  "description": "A GraphQL network inspector for Chrome DevTools with real-time monitoring, syntax highlighting, and batch export.",
  "homepage_url": "https://github.com/da-troll/GraphQeLves",
  "permissions": [
    "storage",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "devtools_page": "devtools.html"
}